{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.webp",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "constants/**/*",
    "assets/**/*"
  ]
}
